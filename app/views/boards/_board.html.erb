<div id="<%= dom_id board%>">
  Board: 
  <span class="font-bold italic"><%= board.board_name %></span>
  by <%= board.email %>
</div>
<div class="mt-2 whitespace-nowrap overflow-x-auto">
  <% mine_board.each do |location| %>
  <div>
    <% location.each do |cell| %>
      <% if cell == 1 %>
          💣
      <% else %>
          ◻️
      <% end %>
    <% end %>
  </div>
  <% end %>
</div>
