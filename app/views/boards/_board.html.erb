<div id="<%= dom_id board %>">
    Board: <%= board.board_name %>
     by <%= board.email %>
</div>
<div class="mt-2">
  <% @board.mine_locations.each do |location| %>
  <div>
    <% location.each do |cell| %>
      <% if cell == 1 %>
          💣
      <% else %>
          ◻️
      <% end %>
    <% end %>
  </div>
  <% end %>
</div>





 

